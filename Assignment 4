8.
include <stdio.h>

int main() {
   int x=1;
   int sqr;
    // DEEP CODE
while(x<=10)
{
    sqr=x*x;
    printf("%d\n",sqr);
    x++;
} 

    return 0;
}

9.
include <stdio.h>

int main() {
   int x=1;
   int cub;
    // DEEP CODE
while(x<=10)
{
    cub=x*x*x;
    printf("%d\n",cub);
    x++;
} 

    return 0;
}

