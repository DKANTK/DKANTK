while(x<=10)
{
    printf("%d\n",x);
    x++;
} 

    return 0;
}

3.
include <stdio.h>

int main() {
   int x=10;
    // DEEP CODE
while(x>=1)
{
    printf("%d\n",x);
    x--;
} 

    return 0;
}

